package com.kodnest.training.pattern.level4;

public class Pattern8 {

	public static void main(String[] args) {
		for(int i=1;i<=5;i++)
		{
			for(int j=5;j>=5;j--)
			{
				System.out.print(" ");
			}
			for(int k=5;k>=5;k--) 
			{
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
